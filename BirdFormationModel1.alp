					<Action><![CDATA[double speed = Speed / 5; // Adjust speed for smoothness
double angle = Heading_Direction; // Use the Heading_Direction parameter value

// Only occasionally update heading direction to avoid erratic movement
if (time() % 5 < 0.1) { // Update direction every 5 seconds
    Heading_Direction += uniform(-Math.PI / 16, Math.PI / 16); // Smooth, small change
}

// Calculate new x and y positions
double newX = getX() + speed * Math.cos(angle);
double newY = getY() + speed * Math.sin(angle);

// Set environment boundaries
double environmentWidth = 1000; // Width of the environment
double environmentHeight = 1000; // Height of the environment

// Adjust the heading direction smoothly when near boundaries
if (newX > environmentWidth || newX < 0) {
    Heading_Direction += Math.PI / 4; // Turn 45 degrees to avoid the boundary
}

if (newY > environmentHeight || newY < 0) {
    Heading_Direction += Math.PI / 4; // Turn 45 degrees to avoid the boundary
}

// Move to the new position
moveTo(newX, newY);
]]></Action>
