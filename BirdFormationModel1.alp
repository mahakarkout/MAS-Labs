						<RecurrenceCode Class="CodeUnitValue">
							<Code><![CDATA[0.1]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</RecurrenceCode>
						<Condition><![CDATA[false]]></Condition>
					</Properties>
					<Action><![CDATA[// Get the Leader position and direction
double leaderX = leader.getX();
double leaderY = leader.getY();
double leaderAngle = leader.Heading_Direction; // Leader's current direction

// Determine whether this Follower is on the left or right side in the V formation
int index = getIndex(); // Get the index of this follower in the population
double side = (index % 2 == 0) ? 1 : -1; // Alternate between left (1) and right (-1)

// Calculate the offset position based on the leader's position
double offsetDistance = distanceToLeader * (index / 2 + 1); // Increase distance for each row
double offsetAngle = leaderAngle + (angleOffset * side);

double targetX = leaderX - offsetDistance * Math.cos(offsetAngle);
double targetY = leaderY - offsetDistance * Math.sin(offsetAngle);

// Introduce a smoothing factor for movement towards the target
double smoothingFactor = 0.1; // Lower values mean more smoothing

// Smoothly move towards the target position
double currentX = getX();
double currentY = getY();
double newX = currentX + (targetX - currentX) * smoothingFactor; // Interpolate between current and target
double newY = currentY + (targetY - currentY) * smoothingFactor; // Interpolate between current and target

// Update the follower's rotation to match the leader's direction smoothly
double currentAngle = getRotation(); // Get current rotation of follower
double rotationSmoothingFactor = 0.05; // Smooth the rotation changes, lower is smoother
double newAngle = currentAngle + (leaderAngle - currentAngle) * rotationSmoothingFactor;

// Move the follower to the new position
moveTo(newX, newY);

// Set the follower's rotation smoothly
setRotation(newAngle);
]]></Action>
